{
  "name": "seo-automation-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:integration": "jest --testPathPattern=integration",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:smoke": "playwright test --grep @smoke",
    "prepare": "husky",
    "migrate": "node scripts/run-migrations.js",
    "db:setup": "npm run migrate",
    "db:migrate:test": "node scripts/run-migrations.js --env=test",
    "db:migrate:staging": "node scripts/run-migrations.js --env=staging",
    "db:migrate:prod": "node scripts/run-migrations.js --env=production",
    "db:check-migrations": "node scripts/check-migrations.js",
    "deployment:backup": "node scripts/deployment-backup.js",
    "deployment:rollback": "node scripts/deployment-rollback.js",
    "deployment:health-check": "node scripts/health-check.js",
    "analyze": "ANALYZE=true npm run build",
    "build:analyze": "cross-env ANALYZE=true npm run build",
    "performance:baseline": "node performance/performance-test-runner.js --test=baseline --env=staging",
    "performance:load": "node performance/performance-test-runner.js --test=load --env=staging",
    "performance:stress": "node performance/performance-test-runner.js --test=stress --env=staging",
    "performance:spike": "node performance/performance-test-runner.js --test=spike --env=staging",
    "performance:endurance": "node performance/performance-test-runner.js --test=endurance --env=staging",
    "performance:suite": "node performance/performance-test-runner.js --suite --env=staging",
    "performance:suite:production": "node performance/performance-test-runner.js --suite --env=production",
    "performance:local": "node performance/performance-test-runner.js --test=load --env=local",
    "performance:validate": "node performance/performance-test-runner.js --test=baseline --env=staging && echo 'Performance validation passed'",
    "db:optimize": "node scripts/apply-database-optimizations.js",
    "db:optimize:dry-run": "node scripts/apply-database-optimizations.js --dry-run",
    "db:optimize:verbose": "node scripts/apply-database-optimizations.js --verbose",
    "db:performance:stats": "node scripts/get-performance-stats.js",
    "db:maintenance": "node scripts/database-maintenance.js",
    "cache:status": "node scripts/cache-management.js status",
    "cache:report": "node scripts/cache-management.js report",
    "cache:invalidate": "node scripts/cache-management.js invalidate all",
    "cache:warm": "node scripts/cache-management.js warm all",
    "cache:maintenance": "node scripts/cache-management.js maintenance",
    "cache:analyze": "node scripts/cache-management.js analyze",
    "cache:openai": "node scripts/cache-management.js status openai",
    "cache:firecrawl": "node scripts/cache-management.js status firecrawl",
    "image:setup": "node scripts/image-optimization-setup.js",
    "image:setup:cloudflare": "node scripts/image-optimization-setup.js cloudflare production",
    "image:setup:aws": "node scripts/image-optimization-setup.js aws production",
    "image:setup:vercel": "node scripts/image-optimization-setup.js vercel production",
    "image:optimize": "node scripts/image-optimization-setup.js --optimize",
    "image:analyze": "node scripts/image-optimization-setup.js --analyze",
    "image:cleanup": "node scripts/image-optimization-setup.js --cleanup",
    "image:status": "node scripts/image-optimization-setup.js --status",
    "image:metrics": "node scripts/image-optimization-setup.js --metrics",
    "image:report": "node scripts/image-optimization-setup.js --report",
    "memory:setup": "node scripts/memory-monitoring-setup.js",
    "memory:setup:production": "node scripts/memory-monitoring-setup.js production",
    "memory:setup:staging": "node scripts/memory-monitoring-setup.js staging",
    "memory:setup:development": "node scripts/memory-monitoring-setup.js development",
    "memory:status": "node scripts/memory-status.js",
    "memory:cleanup": "node scripts/memory-cleanup.js",
    "memory:health": "node scripts/memory-status.js --health",
    "memory:report": "node scripts/memory-status.js --report",
    "memory:gc": "node --expose-gc scripts/memory-cleanup.js --force-gc",
    "memory:leaks": "node scripts/memory-status.js --leaks",
    "memory:performance": "node scripts/memory-status.js --performance",
    "scaling:setup": "node scripts/auto-scaling-setup.js",
    "scaling:setup:production": "node scripts/auto-scaling-setup.js production docker",
    "scaling:setup:aws": "node scripts/auto-scaling-setup.js production aws",
    "scaling:setup:gcp": "node scripts/auto-scaling-setup.js production gcp",
    "scale": "node scripts/auto-scaling.js",
    "scale:status": "node scripts/auto-scaling.js status",
    "scale:up": "node scripts/auto-scaling.js scale-up",
    "scale:down": "node scripts/auto-scaling.js scale-down",
    "scale:traffic": "node scripts/auto-scaling.js traffic",
    "scale:predict": "node scripts/auto-scaling.js predict",
    "scale:rules": "node scripts/auto-scaling.js rules",
    "scale:health": "node scripts/auto-scaling.js health",
    "scale:test": "node scripts/auto-scaling.js test"
  },
  "dependencies": {
    "@google-cloud/language": "^7.2.0",
    "@hookform/resolvers": "^5.1.1",
    "@mendable/firecrawl-js": "^1.29.1",
    "@next/bundle-analyzer": "^15.4.1",
    "@radix-ui/react-checkbox": "^1.3.2",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@sentry/nextjs": "^9.39.0",
    "@supabase/ssr": "^0.6.1",
    "@supabase/supabase-js": "^2.51.0",
    "@upstash/ratelimit": "^2.0.5",
    "@upstash/redis": "^1.35.1",
    "@vercel/analytics": "^1.5.0",
    "@vercel/speed-insights": "^1.2.0",
    "axios": "^1.10.0",
    "bcryptjs": "^2.4.3",
    "cheerio": "^1.1.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "compromise": "^14.14.4",
    "date-fns": "^4.1.0",
    "dotenv": "^16.6.1",
    "express": "^4.19.2",
    "formidable": "^3.5.1",
    "ioredis": "^5.6.1",
    "isomorphic-dompurify": "^2.26.0",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "lucide-react": "^0.525.0",
    "multer": "^1.4.5-lts.1",
    "natural": "^8.1.0",
    "next": "15.4.1",
    "openai": "^5.9.2",
    "perf_hooks": "^0.0.1",
    "quill": "^2.0.3",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.60.0",
    "react-quill": "^2.0.0",
    "sentiment": "^5.0.2",
    "sharp": "^0.33.1",
    "tailwind-merge": "^3.3.1",
    "turndown": "^7.2.0",
    "web-vitals": "^5.0.3",
    "webpack-bundle-analyzer": "^4.10.2",
    "xml2js": "^0.6.2",
    "zod": "^3.23.8",
    "zustand": "^5.0.6"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,css,md}": [
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@faker-js/faker": "^9.9.0",
    "@opentelemetry/context-async-hooks": "^2.0.1",
    "@opentelemetry/core": "^2.0.1",
    "@opentelemetry/instrumentation": "^0.203.0",
    "@opentelemetry/sdk-trace-base": "^2.0.1",
    "@playwright/test": "^1.54.1",
    "@tailwindcss/postcss": "^4",
    "@tanstack/react-query": "^5.83.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/formidable": "^3.4.5",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/lodash": "^4.17.20",
    "@types/multer": "^1.4.12",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "cross-env": "^7.0.3",
    "eslint": "^9",
    "eslint-config-next": "15.4.1",
    "husky": "^9.1.7",
    "jest": "^30.0.4",
    "jest-environment-jsdom": "^30.0.4",
    "lint-staged": "^16.1.2",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "supabase": "^2.31.4",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
